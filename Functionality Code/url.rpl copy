url = {scheme (://) authority path query fragment}

scheme = "http"
authority = [[a-z][A-Z][0-9][:][.][ ][\-]]+
path = [[a-z][A-Z][0-9][:][.][\][ ][\-]]+
query = [[a-z][A-Z][0-9][:][.][ ][\-]]+
fragment = [[a-z][A-Z][0-9][:][.][ ][\-]]+

